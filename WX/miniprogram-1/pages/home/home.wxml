<!-- 轮播图 -->

<swiper indicator-dots circular autoplay>
  <swiper-item wx:for="{{sList}}" wx:key="id">
    <image src="{{item.image}}" mode=""/>
  </swiper-item>
</swiper>

<!-- 9 -->
<view class = "gridList">
<navigator class = "gridItem" wx:for="{{gList}}" wx:key="id" url ="/pages/shopList/shopList?id={{item.id}}&title={{item.name}}">
<image src="{{item.icon}}" mode=""/>
<text>{{item.name}}</text>
</navigator>
</view>

<!--图片区域-->
<view class="img-box">
  <image src="/images/link-01.png"></image>
  <image src="/images/link-02.png"></image>
</view>